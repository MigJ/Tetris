char	*my_strcpy(char *dest, char *src)
{
  int	i;
  int	k;

  i = my_strlen(src);
  k = 0;
  while (k < i)
    {
      dest[k] = src[k];
      k++;
    }
  dest[k] = '\0';
  return (dest);
}
